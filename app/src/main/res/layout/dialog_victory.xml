<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#303651"
        android:gravity="center"
        android:text="Victory"
        android:textColor="#ffff"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:fontFamily="@font/fontdiner_swanky"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <TextView
        android:id="@+id/tvDiemthang"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="ac"
        android:textColor="#ffff"
        android:textSize="20dp"
        android:gravity="center"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:layout_marginTop="32dp"
        app:fontFamily="@font/fontdiner_swanky"
        android:background="@drawable/custom_ketquadialog" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="horizontal">

        <Button

            android:id="@+id/btnChoiTiep"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:text="ChÆ¡i Tiep"
            android:textColor="#fff"
            app:fontFamily="@font/fontdiner_swanky"
            android:background="@drawable/custom_ketquadialog"/>

        <Button
            android:layout_marginLeft="20dp"
            android:id="@+id/btnCancleHomeVictory"
            android:layout_width="120dp"
            android:text="Home"
            app:fontFamily="@font/fontdiner_swanky"
            android:textColor="#fff"
            android:layout_height="40dp"
            android:background="@drawable/custom_ketquadialog"/>


    </LinearLayout>

</LinearLayout>
